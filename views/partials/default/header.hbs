  <style>
    label>input {
      /* HIDE RADIO */
      visibility: hidden;
      /* Makes input not-clickable */
      position: absolute;
      /* Remove input from document flow */
    }

    label>input+img {
      /* IMAGE STYLES */
      cursor: pointer;
      border: 5px solid transparent;
    }

    label>input:checked+img {
      /* (RADIO CHECKED) IMAGE STYLES */
      border: 2px solid #336699;
      border-radius: 7px;
    }
  </style>
